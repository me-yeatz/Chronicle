{
  "buildCommand": "npm run build",
  "outputDirectory": "./dist",
  "routes": [
    {
      "src": "^/$",
      "dest": "/landing.html"
    },
    {
      "src": "^/app(/.*)?$",
      "dest": "/index.html"
    },
    {
      "src": "/sw.js",
      "headers": {
        "Cache-Control": "no-cache",
        "Service-Worker-Allowed": "/"
      }
    },
    {
      "src": "/manifest.webmanifest",
      "headers": {
        "Content-Type": "application/manifest+json"
      }
    },
    {
      "src": "^/(.*)",
      "dest": "/$1"
    }
  ]
}
